<strong>Developer Participant Picker</strong>
<ul>
  <% Participant.all.each do |p| %>
    <li>
      <% if Current.user == p %>
        <strong><%= p.name %></strong>
      <% else %>
        <%= link_to "#{p.name}", impersonate_path(participant_id: p.id), data: {turbo_method: :post} %>
      <% end %>
    </li>
  <% end %>
</ul>